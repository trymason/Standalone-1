{
  "name": "react-standalone",
  "version": "0.3.1",
  "description": "Create framework agnostic components that are truly reusable and interoperable with all the benefits of the React ecosystem â€“ using the HTML5 custom elements API to extend HTML's vocabulary.",
  "main": "dist/standalone.js",
  "scripts": {
    "build": "webpack && npm run dev",
    "test": "ava tests/*.test.js --tap | tap-nyan",
    "dev": "webpack --config webpack-dev.config.js",
    "cleanup": "rimraf node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wildhoney/Standalone.git"
  },
  "ava": {
    "files": [
      "tests/*.test.js"
    ],
    "babel": "inherit",
    "require": [
      "babel-register",
      "./tests/helpers/setup-browser-env.js",
      "./tests/helpers/mock-request.js",
      "document-register-element",
      "./example/packages/mars-weather/index.js"
    ]
  },
  "keywords": [
    "react",
    "standalone",
    "webcomponents",
    "web components",
    "custom element",
    "custom elements",
    "custom",
    "html5",
    "register element",
    "custom element api",
    "component",
    "components"
  ],
  "author": "Adam Timberlake <adam.timberlake@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wildhoney/Standalone/issues"
  },
  "homepage": "https://github.com/Wildhoney/Standalone#readme",
  "devDependencies": {
    "ava": "^0.14.0",
    "axios": "^0.9.1",
    "babel": "^6.5.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "bower": "^1.7.9",
    "document-register-element": "^0.5.4",
    "express": "^4.13.4",
    "humps": "^1.0.0",
    "jsdom": "^8.3.1",
    "moment": "^2.12.0",
    "rimraf": "^2.5.2",
    "tap-nyan": "0.0.2",
    "webpack": "^1.12.15"
  },
  "dependencies": {
    "osom": "^1.1.5",
    "ramda": "^0.21.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  }
}
